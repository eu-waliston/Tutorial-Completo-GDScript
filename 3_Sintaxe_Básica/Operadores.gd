# Aritméticos
var a = 10 + 5    # 15
var b = 10 - 5    # 5
var c = 10 * 5    # 50
var d = 10 / 5    # 2
var e = 10 % 3    # 1
var f = 2 ** 3    # 8

# Comparação
var is_equal = (10 == 10)        # true
var not_equal = (10 != 5)        # true
var greater = (10 > 5)           # true
var less = (5 < 10)              # true

# Lógicos
var and_result = (true and false)    # false
var or_result = (true or false)      # true
var not_result = not true            # false